local animation = Window:MakeTab({
	Name = "Animation shit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

animation:AddButton({
	Name = "Animation script loop",
	Callback = function()
        _G.Loop = true --true if enable loop false if disable the loop
        coroutine.wrap(function()
        while _G.Loop do wait(1)
        coroutine.wrap(function()
        while _G.Loop do wait(1)
        local Animate = game.Players.LocalPlayer.Character.Animate
        Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
        Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=1083218792"
        end
        end)()
        end
        end)()
  	end    
})
animation:AddButton({
	Name = "Disable animation script disable loop",
	Callback = function()
        _G.Loop = false --true if enable loop false if disable the loop
        coroutine.wrap(function()
        while _G.Loop do wait(1)
        coroutine.wrap(function()
        while _G.Loop do wait(1)
        local Animate = game.Players.LocalPlayer.Character.Animate
        Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
        Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=1083218792"
        end
        end)()
        end
        end)()
  	end    
})
